<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href='https://fonts.googleapis.com/css?family=Roboto:400,100,300,700' rel='stylesheet' type='text/css'>
	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
  <link rel="stylesheet" href="/views/css/table.css">

  <title></title>
</head>
<body>
  <h1></h1>
  <section class="ftco-section">
  <div class="container">
    <div class="row justify-content-center">
      <div class="col-md-6 text-center mb-5">
        <h2 class="heading-section">Listagem de Metas</h2>
      </div>
    </div>
    <div class="row">
      <div class="col-md-12">
        <div class="table-wrap">
          <table  id="userTable" border="1" class="table">
            <thead class="thead-primary">
      <tr>
        <th>codven</th>
        <th>dtmeta</th>
        <th>meta</th>
        <th>tipometa</th>
        <th>editar/deletar</th>
      </tr>
    </thead>
    <tbody>
      <% metas.forEach(meta => { %>
        <tr>
          <td><%= meta.codven %></td>
          <td><%= meta.dtmeta %></td>
          <td><%= meta.meta %></td>
          <td><%= meta.tipometa %></td>
          <td><button class="thead-primary" onclick="deleteMeta('<%= meta.codven %>')">Delete</button></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</div>
</div>
</div>
</div>
<script src="/scripts/jquery.min.js"></script>
  <script src="/scripts/popper.js"></script>
  <script src="/scripts/bootstrap.min.js"></script>
  <script src="/scripts/delete.js"></script>
  
</body>
</section>
</html>


